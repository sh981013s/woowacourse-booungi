{
  "compileOnSave": true,
  "compilerOptions": {
    "incremental": true,
    "target": "ES2020",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "outDir": "dist",
    "allowJs": true,
    "sourceMap": false,
    "noEmit": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "../..",
    "paths": {
    }
  },
  "include": [
    "level1-lunch/src/**/*",
    "level1-lunch/__tests__/**/*"
  ],
  "exclude": ["node_modules",
    "cypress",
    "level1-lunch/dist"
  ]
}
